{
  "name": "@hemilabs/react-hooks",
  "version": "1.0.1",
  "description": "Set of reusable hooks for Hemilabs apps with React and Typescript",
  "keywords": [
    "hemilabs",
    "hooks",
    "react",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/hemilabs/react-hooks/issues"
  },
  "license": "MIT",
  "author": {
    "email": "gonzalo@hemi.xyz",
    "name": "Gonzalo D'Elia"
  },
  "files": [
    "LICENSE",
    "README.md",
    "_esm",
    "_types"
  ],
  "repository": "hemilabs/react-hooks",
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm && pnpm run build:types",
    "build:esm": "tsc --noEmit false --outDir ./_esm --sourceMap",
    "build:types": "tsc --noEmit false --declarationDir ./_types --emitDeclarationOnly --declaration --declarationMap",
    "clean": "rm -rf ./_esm ./_types",
    "deps:check": "knip",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@hemilabs/wallet-watch-asset": "^1.0.2",
    "@tanstack/react-query": "5.90.20",
    "@tsconfig/vite-react": "^7.0.2",
    "@types/react": "^19.2.10",
    "@wagmi/core": "^2.19.5",
    "better-sort-github-actions": "^1.0.0",
    "better-sort-package-json": "^1.1.1",
    "commitlint-config-bloq": "^1.1.0",
    "eslint": "^8.57.1",
    "eslint-config-bloq": "^4.7.0",
    "husky": "^9.1.7",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "typescript": "^5.9.3",
    "viem": "^2.45.0",
    "viem-erc20": "^2.1.0",
    "wagmi": "^2.19.5"
  },
  "peerDependencies": {
    "@hemilabs/wallet-watch-asset": "^1",
    "@tanstack/react-query": "^5",
    "react": "^19",
    "viem": "^2",
    "viem-erc20": "^2",
    "wagmi": "^2"
  },
  "packageManager": "pnpm@10.28.1",
  "type": "module",
  "exports": {
    "./*": {
      "types": "./_types/*.d.ts",
      "import": "./_esm/*.js"
    }
  },
  "sideEffects": false
}
